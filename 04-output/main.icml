<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Header3 (unnumbered)">
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <HyperlinkTextDestination Self="HyperlinkTextDestination/#evolution-of-community-discussion-platforms" Name="Destination" DestinationUniqueKey="1" />
