@misc{schwartzZoomInterviewAlpha2023,
  title = {Zoom {{Interview}} with {{Alpha}} Forum Participant {{Jacok Schwartz}} (Benelek) on {{Processing Project}} - 2023-10-25},
  author = {Udvari, Tibor},
  editora = {Schwartz, Jacob},
  editoratype = {collaborator},
  date = {2023-10-25},
  abstract = {In this interview, Tibor Udvari delves into the personal journey of Jacob Schwartz and his early involvement with the Processing project, a cornerstone of the creative coding movement. Schwartz recounts his discovery of Processing while studying architecture in Sydney, expressing a profound interest in programming and visual arts. He reflects on the formative years of his twenties, where the intersection of art, technology, and community played a pivotal role in his development. The conversation touches upon the historical context of web development and the online communities of the early 2000s, highlighting the collaborative spirit and educational value of Processing's ecosystem. Schwartz shares insights into the project's evolution, the influence of its founders, and his transition from architecture to leading a software engineering department. This dialogue sheds light on the motivations behind contributing to open-source projects, the social zeitgeist of the era, and the enduring impact of Processing on both personal and collective levels within the digital art space.},
  langid = {english}
}

