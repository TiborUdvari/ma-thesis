@misc{marxerZoomInterviewLibrary2023,
  title = {Zoom {{Interview}} with Library Contributor {{Ricard Marxer}} ({{Geomerative}}) on the {{Processing Project}} - 2023-10-26},
  editora = {Marxer, Ricard},
  editoratype = {collaborator},
  date = {2023-10-26},
  abstract = {This interview with Ricard Marxer explores his early engagement with the Processing project during his Master's in Arts and Digital Arts around 2004. Ricard recounts his transition from communications engineering to digital arts, driven by a desire to merge artistic expression with coding. He describes his initial frustration with available tools and how discovering Processing fulfilled his need for a code-centric graphics environment, allowing him to delve into vector graphics and Bezier curves without excessive boilerplate code. During the interview, Ricard reminisces about his academic journey, his unexpected shift from a potential research career to digital arts due to administrative hurdles, and his continued passion for combining technology with art. He also discusses the development of his 'Geomerative' library for Processing, designed to manipulate typography vectors, which emerged from his master's thesis. Ricard reflects on the evolution of open-source contributions, his interactions with the Processing community through forums, and the support he received from other artists and developers. He touches on the importance of open-source as a collaborative and educational tool and acknowledges how his contributions led to opportunities for teaching and connecting with like-minded individuals. Furthermore, Ricard shares his perspective on the growth of Processing, its leadership dynamics, and the challenges of managing open-source projects, especially as they scale. The interview concludes with Ricard expressing a renewed interest in generative art amidst the rise of generative AI, inspired by his past work and current technological trends.},
  langid = {english}
}

