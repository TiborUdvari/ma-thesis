@misc{greenwoldZoomInterviewEarly2023,
  title = {Zoom {{Interview}} with Early Contributor and {{ACG}} Member {{Simon Greenwold}} - 2023-10-27},
  author = {Udvari, Tibor},
  editora = {Greenwold, Simon},
  editoratype = {collaborator},
  date = {2023-10-27},
  abstract = {In an insightful interview with Simon Greenwold, an early contributor to the Processing project, he reflects on the project’s inception and its evolution. Greenwold was one of the few who contributed core code in the project’s early stages, including significant rewrites of lighting and camera functionalities. He clarifies misconceptions about the extent of his contributions and the project's use of version control systems, noting the migration from Subversion to Git and the impact this had on the visibility of contributions. Greenwold provides context on the Processing project's roots in the Aesthetics and Computation Group (ACG) at MIT, where the goal was to simplify computational design to its essence. He discusses the dynamics within ACG, led by John Maeda, and its commitment to non-engineers in the design community, a stark contrast to the engineer-heavy teams typical of similar projects. He touches on the challenges of developing Processing in a pre-Git era and the subsequent import of its history into Git repositories. The interview also delves into the pedagogical use of Processing, with Greenwold sharing experiences of teaching computational design at Yale using Processing. He highlights the project's influence on students new to programming and the possibilities it opened for creative exploration. The discussion turns to the importance of Processing's open-source nature and the motivations behind contributors' engagement with the project. Simon Greenwold also reminisces about the early days of web-based deployment via Java applets, acknowledging the critical role this played in the project's dissemination and the ability to showcase work online. He acknowledges the astonishment at Processing's global impact, something neither he nor the project's founders could have anticipated. Lastly, Greenwold touches on the sustainability of Processing, expressing confidence in its continuity and admiration for Ben Fry's ongoing stewardship. The interview concludes with Greenwold expressing a keen interest in the outcomes of the thesis and a willingness to share visual materials from the ACG days to support the research. This interview provides a rich historical perspective on the Processing project, revealing the depth of commitment from its early contributors and the unique blend of technical innovation and creative design that has sustained its growth and relevance.}
}

