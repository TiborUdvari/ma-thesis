@misc{schlegelZoomInterviewControlP52023,
  title = {Zoom Interview with {{ControlP5}}, {{oscP5}} Library Contributor {{Andreas}}\hspace{0pt} {{Schlegel}} on the {{Processing Project}} 2023-10-31},
  author = {Udvari, Tibor},
  editora = {Schlegel, Andreas},
  editoratype = {collaborator},
  date = {2023-10-31},
  abstract = {In this insightful interview, Ricard Marxer discusses his early encounters with the Processing community, tracing back to his academic tenure at the University of California, Santa Barbara, around 2002-2003. With a background in Media Arts and Technology, Marxer recounts the limited tools available to artists and designers at that time, such as Flash and Director, which were predominant before the advent of Processing. Marxer's initial foray into Processing was out of necessity during his final year project where he developed the OSC P5 library, influenced by the creative environment at UCSB that blended visual arts with music. The interview delves into the hybrid program at UCSB, which was instrumental in Marxer's exploration of Open Sound Control and interfacing different applications, leading to his later work on Control P5. This library emerged from Marxer's desire to have a more efficient workflow, enabling control over visuals without constant code alterations. After his academic journey, Marxer's professional trajectory took him to Berlin, where he worked for Art+Com and further developed Control P5 in his spare time. The discussion touches upon the generative design and the art community's curiosity around Processing during the early stages. Marxer reflects on the community forums, his contributions, the evolution of library sharing, and the shifts from a more casual to a formalized process. Marxer also shares insights into the technical infrastructure of the time, the absence of version control, and the reliance on personal management of code versions, contrasting with today's GitHub-centric practices. His journey with Processing and its libraries highlights a self-taught path through the evolving landscape of design and code, leading to current teaching practices with p5.js. The interview concludes with Marxer's observations on the enduring nature of Processing, its educational impact, and its role in the history of computational design. Despite facing criticism for performance issues, Processing's accessibility and simplicity continue to make it a foundational tool for many in the field. Marxer expresses interest in the future of computational design tools and their integration into various industries, and he commends Tibor Udvari's efforts in documenting the history and influence of Processing through his research.},
  langid = {english}
}

@misc{schmidtZoomInterviewCode2023,
  title = {Zoom Interview with Code and Library Contributor {{Karsten Schmidt}} on the {{Processing Project}} - 2023-11-07},
  editora = {Schmidt, Karsten},
  editoratype = {collaborator},
  date = {2023-11-07},
  abstract = {This is an interview with Karsten Schmidt, a former processing contributor and developer of ToxicLibs, a collection of open source libraries for computational design. Schmidt shares his criticisms of processing as a tool and a community, and how he thinks it has hindered innovation and creativity in the field of generative art. He argues that processing has focused too much on beginners and screen-based graphics, and has not embraced the full potential of the Java language or other computational concepts. He also compares processing with other tools, such as Blender, Clojure, and TypeScript, and explains why he left processing behind and explored other paradigms of coding. He also reflects on the role of institutional support and the challenges of open source development.},
  langid = {english}
}

